<div class="padding-global"><div class="container-large"><div class="padding-section-medium"><div class="max-width-large align-center text-align-center"><h2 class="heading-style-h2">
{{ section.settings.text_headline }}
</h2>
<div class="spacer-large"></div></div><div class="marken_list">{% for block in section.blocks %}
    {% if block.type == "logo" %}
                    <div li-block="Logo" class="marken_item">
                                    
                                    <a href="{{ block.settings.url_link }}" class="marken_link w-inline-block"> <img src="{{ block.settings.image_logo | img_url: 'master' }}" loading="lazy" alt="{{ block.settings.image_logo.alt }}" class="marken_logo"> </a>

                                    
                            </div>
            {% endif %}
{% endfor %}
</div></div></div></div> {% schema %} {"tag":"section","name":"Marken","class":"section_marken background-color-grey hide","blocks":[{"name":"Logo","type":"logo","settings":[{"id":"url_link","type":"url","label":"Link"},{"id":"image_logo","type":"image_picker","label":"Logo"}]}],"settings":[{"id":"text_headline","type":"text","label":"Headline","default":"Unsere Top-Marken"}],"presets":[{"name":"Marken","category":"Liquify","blocks":[{"type":"logo"}]}]} {% endschema %}